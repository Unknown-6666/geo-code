
# Discord Bot Configuration
DISCORD_TOKEN=your_discord_token_here

# AI API Keys
AIML_API_KEY=your_aiml_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Google Cloud / Vertex AI Configuration
GOOGLE_CLOUD_PROJECT=your_project_id
VERTEX_LOCATION=us-central1
USE_VERTEX_AI=false
GOOGLE_APPLICATION_CREDENTIALS=key.json

# Database Configuration
DATABASE_URL=your_database_url_here
PGHOST=your_pg_host
PGPORT=5432
PGUSER=your_pg_user
PGPASSWORD=your_pg_password
PGDATABASE=your_pg_database

# Bot Configuration
DEFAULT_PREFIX=!
DEPLOYMENT=false
SYNC_COMMANDS_ON_STARTUP=false
USE_AI_FALLBACK=false
FORCE_COMMAND_REFRESH=false

# Bot Owner Configuration (comma-separated IDs)
BOT_OWNER_IDS=your_user_id,another_user_id
MOD_ROLE_IDS=mod_role_id,another_mod_role_id
JOG_ALLOWED_USER_ID=allowed_user_id
FUN_COMMAND_ALLOWED_IDS=allowed_user_id

# Web Server Configuration
PORT=5000
FLASK_ENV=production
